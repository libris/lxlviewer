{
  "name": "vue-client",
  "version": "1.14.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --mode e2e",
    "test:e2e_ci": "vue-cli-service test:e2e --mode e2e --headless"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "autosize": "^4.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.7.tar.gz",
    "combokeys": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "font-awesome": "4.7.0",
    "kungbib-styles": "^1.3.3",
    "less": "^3.10.3",
    "less-loader": "^4.1.0",
    "lodash-es": "^4.17.14",
    "marked": "^0.5.2",
    "md5": "^2.2.1",
    "modernizr": "^3.6.0",
    "moment": "^2.22.2",
    "portal-vue": "^1.5.0",
    "style-resources-loader": "^1.3.2",
    "v-tooltip": "^2.0.2-rc.33",
    "vue": "^2.6.10",
    "vue-cli-plugin-moment": "^0.1.1",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-clickaway": "https://github.com/silverspectro/vue-clickaway/archive/2.2.3.tar.gz",
    "vue-clipboard2": "^0.3.0",
    "vue-router": "^3.1.3",
    "vue-simple-spinner": "^1.2.8",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.2",
    "webpack": "^4.41.2",
    "webpack-modernizr-loader": "^4.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {},
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-use-before-define": "off",
      "no-plusplus": "off",
      "no-restricted-syntax": "off",
      "no-console": "off",
      "no-trailing-spaces": "off",
      "no-prototype-builtins": "off",
      "prefer-destructuring": "off",
      "no-underscore-dangle": "off",
      "max-len": "off",
      "prefer-promise-reject-errors": "off",
      "no-cond-assign": "off",
      "object-shorthand": "off",
      "import/extensions": "off",
      "no-param-reassign": "off",
      "import/no-dynamic-require": "off",
      "object-curly-newline": [
        "error",
        {
          "ImportDeclaration": "never"
        }
      ],
      "global-require": "off",
      "radix": "off",
      "no-alert": "off",
      "no-continue": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
