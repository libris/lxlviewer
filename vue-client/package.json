{
  "name": "vue-client",
  "version": "1.29.4",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "postinstall": "yarn --cwd ../lxljs/ install --frozen-lockfile",
    "lint": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --mode e2e",
    "test:e2e_ci": "vue-cli-service test:e2e --mode e2e --headless"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "autosize": "^4.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.0.0",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.7.tar.gz",
    "client-oauth2": "^4.3.3",
    "combokeys": "https://github.com/avocode/combokeys/archive/v3.1.0.tar.gz",
    "eslint": "^7.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "focus-trap": "^6.0.1",
    "focus-trap-vue": "^1.1.1",
    "font-awesome": "4.7.0",
    "git-describe": "^4.0.4",
    "json-loader": "^0.5.7",
    "kungbib-styles": "^1.3.3",
    "less": "^3.10.3",
    "less-loader": "^4.1.0",
    "lodash-es": "^4.17.14",
    "lxljs": "link:../lxljs",
    "marked": "^2.0.0",
    "md5": "^2.2.1",
    "modernizr": "^3.6.0",
    "moment": "^2.29.4",
    "path": "^0.12.7",
    "portal-vue": "^1.5.0",
    "style-resources-loader": "^1.3.3",
    "v-tooltip": "^2.0.2-rc.33",
    "vue": "^2.6.10",
    "vue-cli-plugin-moment": "^0.1.1",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-clickaway": "https://github.com/silverspectro/vue-clickaway/archive/2.2.3.tar.gz",
    "vue-clipboard2": "^0.3.0",
    "vue-meta": "^2.4.0",
    "vue-resize": "^1.0.1",
    "vue-router": "^3.1.5",
    "vue-simple-spinner": "^1.2.8",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2",
    "webpack": "^4.41.5",
    "webpack-modernizr-loader": "^4.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {},
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "runtimeConfig": "readonly"
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "ignorePatterns": [
      "**/node_modules/**"
    ],
    "rules": {
      "no-use-before-define": "off",
      "no-plusplus": "off",
      "no-restricted-syntax": "off",
      "no-console": "off",
      "no-trailing-spaces": "off",
      "no-prototype-builtins": "off",
      "prefer-destructuring": "off",
      "prefer-object-spread": "off",
      "no-underscore-dangle": "off",
      "max-len": "off",
      "prefer-promise-reject-errors": "off",
      "no-cond-assign": "off",
      "object-shorthand": "off",
      "import/no-cycle": "off",
      "import/extensions": "off",
      "no-param-reassign": "off",
      "import/prefer-default-export": "off",
      "import/no-dynamic-require": "off",
      "object-curly-newline": [
        "error",
        {
          "ImportDeclaration": "never"
        }
      ],
      "global-require": "off",
      "radix": "off",
      "no-alert": "off",
      "no-continue": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "allowImportExportEverywhere": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "globals": {
      "runtimeConfig": {}
    },
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es|lxljs)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
