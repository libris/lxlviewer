@top Query { term* }

@skip { space }

term {
  Qualifier |
  String |
  CompareOperator |
  Group { "(" term* ")" }
}

Qualifier {
  Identifier EqualOperator (Identifier | String | Number | prefix) prefix*
}

@tokens {
  Identifier { $[a-zA-ZåäöÅÄÖ]+ }

  String { '"' (!["\\] | "\\" _)* '"' }

  Number { @digit+ }

  EqualOperator { ":" | "=" }

  CompareOperator { ">" | "<" | ">=" | "<=" }

  prefix { "*"+ }

  space { @whitespace+ }

  "(" ")"
}

@detectDelim
